{
  "editor.formatOnSave": true,
  "cSpell.words": [
    "commitlint",
    "Elems",
    "errno",
    "hoverbar",
    "nocheck",
    "prettierrc",
    "snabbdom",
    "uppy",
    "vnode",
    "wangeditor",
    "wangfupeng"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript"
  ],
  "editor.detectIndentation": false,
  "editor.tabSize": 2
}